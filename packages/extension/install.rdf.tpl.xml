<?xml version="1.0" encoding="UTF-8"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    
    <RDF:Description about="urn:mozilla:install-manifest">

        <em:id>%%ID%%</em:id>
        <em:name>%%Name%%</em:name>
        <em:version>%%Version%%%%Release%%</em:version>
        <em:type>2</em:type>
    
        <em:creator>%%Creator%%</em:creator>
        <em:description>%%Description%%</em:description>

        <em:homepageURL>%%HomepageURL%%</em:homepageURL>
        <em:iconURL>%%IconURL%%</em:iconURL>
        <em:optionsURL>%%OptionsURL%%</em:optionsURL>

        <em:targetApplication>
            <RDF:Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:maxVersion>3.5.*</em:maxVersion>
                <em:minVersion>3.0</em:minVersion>
            </RDF:Description>
        </em:targetApplication>
		
        <!-- NOTE: Do not move these tags above <em:targetApplication>. It will break the registration process! -->
        <em:updateURL>%%UpdateURL%%</em:updateURL>
        <em:updateKey>%%UpdateKey%%</em:updateKey>
        
    </RDF:Description>
</RDF>